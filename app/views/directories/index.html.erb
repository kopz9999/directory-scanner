<%= form_for @directory, url: {action: :search} do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :address %>:
  <%= f.text_field :address %><br />

  <%= f.label :city %>:
  <%= f.text_field :city %><br />

  <%= f.label :state %>:
  <%= f.text_field :state %><br />

  <%= f.label :zip %>:
  <%= f.text_field :zip %><br />

  <%= f.label :phone_number %>:
  <%= f.text_field :phone_number %><br />

  <%= f.submit 'Search' %>
<% end %>
